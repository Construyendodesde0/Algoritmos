algoritmo "CalculoPromedio del alumno"

var
   G1, G2, RES: Inteiro

inicio
      Escreval ("----------FUTBOL ARGENTINO----------")
      Escreva("Cuantos goles hizo Belgrano: ")
      Leia(G1)
      Escreva("Cuantos goles hizo Talleres: ")
      Leia(G2)
      Escreval ("------------------------")
      Escolha RES
              Caso 0
                 RES1

      

fimalgoritmo
